{
  "manifest_version": 3,
  "name": "VIT AP AUTO WIFI LOGIN",
  "description": "AUTO WIFI LOGIN FOR VITIANS",
  "version": "1.0",
  "permissions": ["storage", "scripting", "activeTab"],
  "host_permissions": ["https://hfw.vitap.ac.in/*"],
  "content_scripts": [
    {
      "matches": ["http://172.18.10.10:1000/*"],
      "js": ["scripts/content-script-2.js"]
    },
    {
      "matches": ["https://hfw.vitap.ac.in:8090/*"],
      "js": ["scripts/content-script-1.js"]
    }
  ],
  "background": {
    "service_worker": "scripts/background.js"
  },
  "action": {
    "default_popup": "./popup/popup.html",
    "default_icon": "./images/hello.png"
  },

  "web_accessible_resources": [
    {
      "resources": ["scripts/inject-1.js"],
      "matches": ["https://hfw.vitap.ac.in/*"]
    },
    {
      "resources": ["scripts/inject-2.js"],
      "matches": ["http://172.18.10.10:1000/*"]
    }
  ]
}
